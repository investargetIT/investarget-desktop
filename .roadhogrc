{
  "entry": ["src/index.js", "src/entry/zh-Hans-CN.js", "src/entry/en-US.js"],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
       ["import", { "libraryName": "antd", "style": true }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
       ["import", { "libraryName": "antd", "style": true }]
      ]
    }
  },
  "theme": "./src/theme.js",
  "cssModulesExclude": [
    "./src/base_components/theme.less",
    "./src/base_components/button.less",
    "./src/base_components/menu.less",
    "./src/base_components/index.less"
  ]
}
